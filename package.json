    "@typescript-eslint/parser": "^4.28.5",
    "babel-jest": "^27.1.1",
    "babel-loader": "^8.2.2",
    "clsx": "^1.1.1",
    "eslint": "^7.31.0",
    "jest": "^27.0.6",
    "paratype": "^0.6.10",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-jss": "^10.7.1",
